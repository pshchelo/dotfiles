# setting language to English (US)
LANGUAGE=en_US:en
LANG=en_US.UTF-8
LC_MESSAGES=en_US.UTF-8
LC_ALL=en_IE.UTF-8

# Ireland locale settings for sane European defaults
LC_NUMERIC=en_IE.UTF-8
