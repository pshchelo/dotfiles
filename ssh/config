IdentityFile ~/.ssh/id_ed25519
IdentityFile ~/.ssh/id_rsa
IdentitiesOnly yes

Host swthm
    HostName 192.168.1.11
    IdentityFile ~/.ssh/phys_ed25519

Host devstack
    HostName 192.168.100.10
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    LogLevel QUIET

Host kvm-work
    HostName pshchelokovskyy-pc.kha.mirantis.net
    IdentityFile ~/.ssh/phys_ed25519

Host dsvm1-kvm-work
    HostName 192.168.100.11
    DynamicForward 10011

Host dsvm2-kvm-work
    HostName 192.168.100.12
    DynamicForward 10012

Host dsvm3-kvm-work
    HostName 192.168.100.13
    DynamicForward 10013

Host *-kvm-work
    ForwardAgent yes
    ProxyCommand ssh -W %h:%p kvm-work
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    LogLevel QUIET

Host mos-ironic-storage
    HostName mos-ironic.vm.mirantis.net
    User pshchelokovskyy

Host mos-ironic-lab
    HostName cz7918.bud.mirantis.net
    User pshchelokovskyy

