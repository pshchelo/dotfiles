IdentityFile ~/.ssh/git_ed25519
IdentityFile ~/.ssh/id_ed25519
# Older Gerrit does not accept ed25519 keys
IdentityFile ~/.ssh/git_rsa
#IdentityFile ~/.ssh/id_rsa

Host swthm
    HostName 192.168.1.11

Host devstack
    HostName 192.168.100.10
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    LogLevel QUIET

Host kvm-work
    HostName pshchelokovskyy-pc.kha.mirantis.net

Host dsvm1-kvm-work
    HostName 192.168.100.11
    DynamicForward 10011

Host dsvm2-kvm-work
    HostName 192.168.100.12
    DynamicForward 10012

Host *-kvm-work
    ProxyCommand ssh -W %h:%p kvm-work
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    LogLevel QUIET

Host mos-ironic-storage
    HostName mos-ironic.vm.mirantis.net
    User pshchelokovskyy

Host mos-ironic-lab
    HostName cz7918.bud.mirantis.net
    User pshchelokovskyy

# Some ranges if private IP addresses I use for ephemeral instances
# Virt-manager local networks, DevStack public and private networks
Host 192.168.100.* 192.168.122.* 10.0.*.* 172.24.4.*
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    LogLevel QUIET
